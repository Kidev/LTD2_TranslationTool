{
	"name": "LTD2 Translation Tool",
	"manifest_version": 3,
	"version": "1.5",
	"description": "Adds cool quality of life features to Google Sheet to help translators of Legion TD 2",
	"permissions": [
		"activeTab",
		"scripting"
	],
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"32": "icon/icon-32.png",
		"48": "icon/icon-48.png"
	},
	"action": {
		"default_title": "Toggle Coach's Translation Tool for current tab",
		"default_icon": "icon/icon-48.png"
	},
	"web_accessible_resources": [{
		"resources": ["inject.js"],
		"matches": ["<all_urls>"]
	},{
		"resources": ["preview_box.html"],
		"matches": ["<all_urls>"]
	},{
		"resources": ["fonts/nucleo.woff2", "fonts/pxiByp8kv8JHgFVrLCz7Z1xlFQ.woff2", "fonts/pxiByp8kv8JHgFVrLDz8Z1xlFQ.woff2", "fonts/pxiByp8kv8JHgFVrLEj6Z1xlFQ.woff2", "fonts/pxiEyp8kv8JHgFVrJJfecg.woff2"],
		"matches": ["<all_urls>"]
	},{
		"resources": ["inject.css", "css/btf.css", "css/black-dashboard.css", "css/flag-icon.css", "css/style.css", "css/theme.css", "css/empty.css"],
		"matches": ["<all_urls>"]
	}]
}